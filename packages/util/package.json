{
  "name": "@welshman/util",
  "version": "0.1.0",
  "author": "hodlbod",
  "license": "MIT",
  "description": "A collection of nostr-related utilities.",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "files": [
    "build"
  ],
